{
  "name": "blog",
  "version": "0.0.1",
  "description": "a blog for recording my experiences",
  "author": "zhaojun <j_z@foxmail.com>",
  "private": true,
  "scripts": {
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.config.server.js",
    "clean": "rimraf client-dist && rimraf server-dist",
    "build": "yarn run clean && yarn run build:client && yarn run build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "commit": "npm install -g commitizen && commitizen init cz-conventional-changelog --save-dev --save-exact",
    "lint": "eslint --ext .js --ext .jsx --ext .vue .",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue .",
    "precommit": "lint-staged",
    "start": "cross-env NODE_ENV=production node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhao-jun/blog.git"
  },
  "keywords": [
    "blog"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "element-ui": "^2.4.11",
    "github-markdown-css": "^2.10.0",
    "highlight.js": "^9.13.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "art-template": "^4.12.2",
    "autoprefixer": "^9.1.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^2.4.1",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "4.0.0-beta.5",
    "husky": "^0.14.3",
    "koa": "^2.5.2",
    "koa-art-template": "^1.1.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "marked": "4.0.10",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.2",
    "moment": "^2.23.0",
    "mysql2": "^1.6.4",
    "nodemon": "^1.18.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "sequelize": "^4.42.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.3.0",
    "vue-server-renderer": "^2.5.17",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "*.{js,vue}": [
      "yarn run lint-fix",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/zhao-jun/blog/issues"
  },
  "homepage": "https://github.com/zhao-jun/blog#readme"
}
